<?xml version="1.0"?>
<config>
	<modules>
        <Zozoconcepts_Hybrid>
            <version>1.0.1</version>
        </Zozoconcepts_Hybrid>
    </modules>
	<frontend>
		<routers>
			<hybrid>
				<use>standard</use>
				<args>
					<module>Zozoconcepts_Hybrid</module>
					<frontName>hybrid</frontName>
				</args>
			</hybrid>
		</routers>
		<layout>
			<updates>
				<hybrid>
					<file>local.xml</file>
					<file>responsive.xml</file>
				</hybrid>
			</updates>
		</layout>
	</frontend>
    <admin>
        <routers>
			<hybrid>
				<use>admin</use>
				<args>
					<module>Zozoconcepts_Hybrid</module>
					<frontName>hybrid</frontName>
				</args>
			</hybrid>
        </routers>
    </admin>
	<adminhtml>
		<layout>
			<updates>
				<hybrid>
					<file>hybrid.xml</file>
				</hybrid>
			</updates>
		</layout>
		<events>            	
			<controller_action_postdispatch_adminhtml_system_config_save>
				<observers>
					<hybrid>
						<type>singleton</type>
						<class>hybrid/observer</class>
						<method>hookTo_controllerActionPostdispatchAdminhtmlSystemConfigSave</method>
					</hybrid>
				</observers>
			</controller_action_postdispatch_adminhtml_system_config_save>
			<store_edit>
				<observers>
					<hybrid>
						<type>singleton</type>
						<class>hybrid/observer</class>
						<method>hookTo_storeEdit</method>
					</hybrid>
				</observers>
			</store_edit>
		</events>
	</adminhtml>
	<global>
		<blocks>
            <hybrid>
                <class>Zozoconcepts_Hybrid_Block</class>
            </hybrid>
        </blocks>
		<helpers>
            <hybrid>
                <class>Zozoconcepts_Hybrid_Helper</class>
            </hybrid>
        </helpers>
		<models>
			<core>
                <rewrite>
                    <layout>Zozoconcepts_Hybrid_Model_Core_Layout</layout>
                </rewrite>
            </core>
            <hybrid>
                <class>Zozoconcepts_Hybrid_Model</class>
            </hybrid>
        </models>
		<resources>
			<hybrid_setup>
				<setup>
					<module>Zozoconcepts_Hybrid</module>
					<class>Zozoconcepts_Hybrid_Model_Resource_Mysql4_Setup</class>
				</setup>
				<connection>
					<use>core_setup</use>
				</connection>
			</hybrid_setup>
			<hybrid_write>
				<connection>
					<use>core_write</use>
				</connection>
			</hybrid_write>
			<hybrid_read>
				<connection>
					<use>core_read</use>
				</connection>
			</hybrid_read>
		</resources>
	</global>
